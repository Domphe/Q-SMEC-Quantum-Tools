{"id": "eq.qsmec.nep", "latex": "\\text{NEP} = \\frac{\\text{Noise}}{\\text{Responsivity}} = \\frac{\\sqrt{S_n(f)}}{R} \\ [\\text{W}/\\sqrt{\\text{Hz}}]", "description": "Noise Equivalent Power - minimum detectable signal power", "domain": "quantum_sensing", "variables": [{"symbol": "S_n(f)", "meaning": "Noise power spectral density", "units": "W²/Hz"}, {"symbol": "R", "meaning": "Responsivity", "units": "A/W or V/W"}, {"symbol": "\\text{NEP}", "meaning": "Noise equivalent power", "units": "W/√Hz"}], "refs": []}
{"id": "eq.qsmec.snr_db", "latex": "\\text{SNR}_{\\text{dB}} = 10 \\log_{10}\\left(\\frac{P_{\\text{signal}}}{P_{\\text{noise}}}\\right)", "description": "Signal-to-noise ratio in decibels", "domain": "quantum_sensing", "variables": [{"symbol": "P_{\\text{signal}}", "meaning": "Signal power", "units": "W"}, {"symbol": "P_{\\text{noise}}", "meaning": "Noise power", "units": "W"}, {"symbol": "\\text{SNR}_{\\text{dB}}", "meaning": "Signal-to-noise ratio", "units": "dB"}], "refs": []}
{"id": "eq.qsmec.fom", "latex": "\\text{FOM} = \\frac{R \\times \\Delta f}{\\text{NEP}}", "description": "Figure of Merit for THz sensors", "domain": "quantum_sensing", "variables": [{"symbol": "R", "meaning": "Responsivity", "units": "A/W"}, {"symbol": "\\Delta f", "meaning": "Bandwidth", "units": "Hz"}, {"symbol": "\\text{NEP}", "meaning": "Noise equivalent power", "units": "W/√Hz"}, {"symbol": "\\text{FOM}", "meaning": "Figure of merit", "units": "A·√Hz/W"}], "refs": []}
{"id": "eq.qsmec.responsivity", "latex": "R = \\frac{I_{\\text{signal}}}{P_{\\text{incident}}} \\ [\\text{A}/\\text{W}]", "description": "Detector responsivity - output current per incident power", "domain": "quantum_sensing", "variables": [{"symbol": "I_{\\text{signal}}", "meaning": "Output signal current", "units": "A"}, {"symbol": "P_{\\text{incident}}", "meaning": "Incident optical power", "units": "W"}, {"symbol": "R", "meaning": "Responsivity", "units": "A/W"}], "refs": []}
{"id": "eq.qsmec.mcmillan", "latex": "T_c = \\frac{\\omega_{\\log}}{1.2} \\exp\\left[-\\frac{1.04(1+\\lambda)}{\\lambda - \\mu^*(1+0.62\\lambda)}\\right]", "description": "McMillan equation for superconductor critical temperature", "domain": "quantum_materials", "variables": [{"symbol": "T_c", "meaning": "Critical temperature", "units": "K"}, {"symbol": "\\omega_{\\log}", "meaning": "Logarithmic average phonon frequency", "units": "K"}, {"symbol": "\\lambda", "meaning": "Electron-phonon coupling constant", "units": "dimensionless"}, {"symbol": "\\mu^*", "meaning": "Coulomb pseudopotential", "units": "dimensionless"}], "refs": []}
{"id": "eq.qsmec.seebeck", "latex": "S = -\\frac{1}{e T} \\frac{\\int \\sigma(E) (E-E_F) \\left(-\\frac{\\partial f}{\\partial E}\\right) dE}{\\int \\sigma(E) \\left(-\\frac{\\partial f}{\\partial E}\\right) dE}", "description": "Seebeck coefficient from Boltzmann transport", "domain": "quantum_materials", "variables": [{"symbol": "S", "meaning": "Seebeck coefficient", "units": "V/K"}, {"symbol": "\\sigma(E)", "meaning": "Energy-dependent conductivity", "units": "S"}, {"symbol": "E_F", "meaning": "Fermi energy", "units": "eV"}, {"symbol": "f", "meaning": "Fermi-Dirac distribution", "units": "dimensionless"}, {"symbol": "T", "meaning": "Temperature", "units": "K"}], "refs": []}
{"id": "eq.qsmec.zt", "latex": "ZT = \\frac{S^2 \\sigma T}{\\kappa_e + \\kappa_L}", "description": "Thermoelectric figure of merit", "domain": "quantum_materials", "variables": [{"symbol": "ZT", "meaning": "Dimensionless figure of merit", "units": "dimensionless"}, {"symbol": "S", "meaning": "Seebeck coefficient", "units": "V/K"}, {"symbol": "\\sigma", "meaning": "Electrical conductivity", "units": "S/m"}, {"symbol": "T", "meaning": "Temperature", "units": "K"}, {"symbol": "\\kappa_e", "meaning": "Electronic thermal conductivity", "units": "W/m·K"}, {"symbol": "\\kappa_L", "meaning": "Lattice thermal conductivity", "units": "W/m·K"}], "refs": []}
{"id": "eq.qsmec.complex_permittivity", "latex": "\\varepsilon = \\varepsilon' - i\\varepsilon'' = \\varepsilon_r \\varepsilon_0", "description": "Complex permittivity for EM materials", "domain": "quantum_materials", "variables": [{"symbol": "\\varepsilon'", "meaning": "Real permittivity (energy storage)", "units": "F/m"}, {"symbol": "\\varepsilon''", "meaning": "Imaginary permittivity (loss)", "units": "F/m"}, {"symbol": "\\varepsilon_r", "meaning": "Relative permittivity", "units": "dimensionless"}, {"symbol": "\\varepsilon_0", "meaning": "Vacuum permittivity", "units": "F/m"}], "refs": []}
{"id": "eq.qsmec.rcs", "latex": "\\sigma_{\\text{RCS}} = \\lim_{r \\to \\infty} 4\\pi r^2 \\frac{|\\mathbf{E}_{\\text{scattered}}|^2}{|\\mathbf{E}_{\\text{incident}}|^2}", "description": "Radar cross section for stealth applications", "domain": "quantum_materials", "variables": [{"symbol": "\\sigma_{\\text{RCS}}", "meaning": "Radar cross section", "units": "m²"}, {"symbol": "r", "meaning": "Distance from target", "units": "m"}, {"symbol": "\\mathbf{E}_{\\text{scattered}}", "meaning": "Scattered electric field", "units": "V/m"}, {"symbol": "\\mathbf{E}_{\\text{incident}}", "meaning": "Incident electric field", "units": "V/m"}], "refs": []}
{"id": "eq.qsmec.impedance_match", "latex": "Z = \\sqrt{\\frac{\\mu}{\\varepsilon}} = \\sqrt{\\frac{\\mu_r}{\\varepsilon_r}} Z_0", "description": "Impedance matching for perfect absorption", "domain": "quantum_materials", "variables": [{"symbol": "Z", "meaning": "Wave impedance", "units": "Ω"}, {"symbol": "\\mu", "meaning": "Permeability", "units": "H/m"}, {"symbol": "\\varepsilon", "meaning": "Permittivity", "units": "F/m"}, {"symbol": "Z_0", "meaning": "Free space impedance (377Ω)", "units": "Ω"}], "refs": []}
