{"id": "eq.qsmec.detection_range", "latex": "R_{\\max} = \\left(\\frac{P_t G_t G_r \\lambda^2 \\sigma_{\\text{RCS}}}{(4\\pi)^3 P_{\\min}}\\right)^{1/4}", "description": "Radar detection range equation", "domain": "quantum_sensing", "variables": [{"symbol": "R_{\\max}", "meaning": "Maximum detection range", "units": "m"}, {"symbol": "P_t", "meaning": "Transmit power", "units": "W"}, {"symbol": "G_t", "meaning": "Transmit antenna gain", "units": "dimensionless"}, {"symbol": "G_r", "meaning": "Receive antenna gain", "units": "dimensionless"}, {"symbol": "\\lambda", "meaning": "Wavelength", "units": "m"}, {"symbol": "\\sigma_{\\text{RCS}}", "meaning": "Target RCS", "units": "m²"}, {"symbol": "P_{\\min}", "meaning": "Minimum detectable power", "units": "W"}], "refs": []}
